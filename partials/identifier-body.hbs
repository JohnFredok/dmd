<dl>
  <dt>kind</dt><dd>{{kind}}</dd>
  <dt>description</dt><dd>{{description}}</dd>
  <dt>params</dt><dd>{{#each params}}{{type.names}} {{description}}{{/each}}</dd>
  <dt>examples</dt><dd><code>{{examples}}</code></dd>
  <dt>memberof</dt><dd><code>{{scope}} {{kind}} of {{linkTo memberof}}</code></dd>
  <dt>scope</dt><dd><code>{{scope}}</code></dd>
</dl>