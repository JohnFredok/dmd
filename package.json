{
  "name": "dmd",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "0.2.9",
  "description": "dmd",
  "repository": "https://github.com/75lb/dmd.git",
  "main": "./lib/dmd.js",
  "bin": {
    "dmd": "bin/cli.js"
  },
  "keywords": [
    "documentation",
    "markdown"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "tape test/*.js",
    "lint": "jshint lib/*.js bin/*.js test/*.js helpers/*.js; echo",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs lib/*.js > README.md; echo",
    "watch": "baldrick --do 'npm run html' --when '**/*.hbs' --change --speak;",
    "p1": "jsdoc-parse '../jsdoc-parse/test/fixture/**/*.js' > test/fixture/everything.json",
    "p2": "jsdoc-parse ../file-set/lib/file-set.js > test/fixture/file-set.js",
    "p3": "jsdoc-parse ../handbrake-js/lib/* > test/fixture/handbrake.json",
    "p4": "jsdoc-parse ~/Documents/vendor/lodash/lodash.js > test/fixture/lodash.json",
    "p5": "jsdoc-parse ~/Documents/vendor/famous-map/MapView.js > test/fixture/MapView.json",
    "p6": "jsdoc-parse ~/Documents/vendor/angular-debaser/debaser.js > test/fixture/debaser.json",
    "h1": "cat test/fixture/everything.json | dmd --partial 'html/**/*.hbs' --template html/docs.hbs > test/expected/everything.html;",
    "h2": "cat test/fixture/file-set.json | dmd --partial 'html/**/*.hbs' --template html/docs.hbs > test/expected/file-set.html;",
    "h3": "cat test/fixture/handbrake.json | dmd --partial 'html/**/*.hbs' --template html/docs.hbs > test/expected/handbrake.html;",
    "h4": "cat test/fixture/lodash.json | dmd --partial 'html/**/*.hbs' --template html/docs.hbs > test/expected/lodash.html;",
    "h5": "cat test/fixture/MapView.json | dmd --partial 'html/**/*.hbs' --template html/docs.hbs > test/expected/MapView.html;",
    "m1": "cat test/fixture/everything.json | dmd > test/md/everything.md;",
    "m2": "cat test/fixture/file-set.json | dmd > test/md/file-set.md;",
    "m3": "cat test/fixture/handbrake.json | dmd > test/md/handbrake.md;",
    "m4": "cat test/fixture/lodash.json | dmd > test/md/lodash.md;",
    "m5": "cat test/fixture/MapView.json | dmd > test/md/MapView.md;",
    "t1": "cat test/fixture/everything.json | dmd --template test/template/module.hbs > test/md/module.md",
    "t2": "cat test/fixture/file-set.json | dmd --template test/template/exported-class.hbs > test/md/exported-class.md",
    "t3": "cat test/fixture/everything.json | dmd --template test/template/class.hbs > test/md/class.md",
    "t4": "cat test/fixture/everything.json | dmd --template test/template/modules.hbs > test/md/modules.md",
    "p": "npm run p1; npm run p2; npm run p3; npm run p4; npm run p5; echo",
    "h": "npm run h1; npm run h2; npm run h3; npm run h4; npm run h5; echo",
    "m": "npm run m1; npm run m2; npm run m3; npm run m4; npm run m5; echo",
    "t": "npm run t1; npm run t2; npm run t3; npm run t4; ",
    "all": "npm run p; npm run h; npm run m; npm run t; "
  },
  "dependencies": {
    "array-tools": "^1.3.0",
    "boil-js": "~0.2.0",
    "command-line-args": "~0.5.1",
    "console-dope": "~0.3.6",
    "marked": "~0.3.2",
    "object-tools": "^1.1.1",
    "string-tools": "~0.1.5"
  },
  "devDependencies": {
    "tape": "^3"
  },
  "local-web-server": { "port": 8080 }
}
